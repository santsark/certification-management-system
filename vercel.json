{
    "buildCommand": "npm run build",
    "outputDirectory": ".next",
    "devCommand": "npm run dev",
    "installCommand": "npm install",
    "framework": "nextjs",
    "regions": [
        "iad1"
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "build": {
        "env": {
            "DATABASE_URL": "@database_url",
            "GEMINI_API_KEY": "@gemini_api_key",
            "SESSION_SECRET": "@session_secret"
        }
    }
}